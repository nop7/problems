#include <iostream>
#include <vector>
#include <list>
#include <set>
#include <map>
#include <algorithm>
#include <string>
#include <complex>
#include <cstdio>
using namespace std;

#define REP(i,p,n) for(int i=p;i<n;++i)
#define rep(i,n) REP(i,0,n)

typedef long long int ll;
typedef pair<int,int> pii;
typedef complex<double> point;

int main(
{
	int x,y,s;
	cin>>x>>y>>s;
	x+=100;
	y+=100;

	int res = 0;

	for(int i=1;i<s;++i)
	for(int j=1;j<s;++j)
	{
		int a = (i*x)/100;
		int b = (j*x)/100;
		if( a+b != s ){ continue; }

		res = max(res,((i*y)/100)+((j*y)/100));
	}

	cout << res << endl;

	return 0;
}



